    test("Should run without failures") {

        when {
            params {
                input = "https://github.com/phac-nml/nf-core-staramr/raw/dev/assets/samplesheet.csv"
                outdir = "$baseDir/tests/results"
            }
        }

        then {
            assert workflow.success
        }

    }

}
